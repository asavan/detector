{
  "name": "nedanila",
  "version": "1.0.0",
  "description": "face recognition sample",
  "main": "index.js",
  "author": "asavan",
  "license": "UNLICENSED",
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "train": "node local\\train.js",
    "test": "ava local/rec.test.js --timeout=1m",
    "test-small": "ava local/rec.test.js --timeout=1m --match='once'",
    "test-lavka": "ava local/rec.test.js --timeout=1m --match='yandexlavka'",
    "all": "run-s train test build"
  },
  "config": {
    "mode": "yandexlavka",
    "site": "yandexlavka"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^3.13.0",
    "@tensorflow/tfjs-node": "^3.13.0",
    "@vladmandic/face-api": "^1.6.5",
    "ava": "^4.0.1",
    "canvas": "^2.9.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.6.0",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-check-updates": "^12.3.0",
    "npm-run-all": "^4.1.5",
    "string-replace-loader": "^3.1.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "workbox-webpack-plugin": "^6.4.2"
  }
}
