{
  "name": "nedanila",
  "version": "1.0.0",
  "description": "face recognition sample",
  "main": "index.js",
  "author": "asavan",
  "license": "UNLICENSED",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development",
    "build": "webpack --mode production",
    "train": "node local\\train.js",
    "test": "ava local/rec.test.js --timeout=1m",
    "test-small": "ava local/rec.test.js --timeout=1m --match='once'",
    "test-lavka": "ava local/rec.test.js --timeout=1m --match='yandexlavka'",
    "all": "run-s train test build"
  },
  "config": {
    "mode": "default",
    "site": ""
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^3.20.0",
    "@tensorflow/tfjs-node": "^3.20.0",
    "@vladmandic/face-api": "^1.7.3",
    "ava": "^4.3.3",
    "canvas": "^2.10.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.0",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-check-updates": "^16.3.8",
    "npm-run-all": "^4.1.5",
    "string-replace-loader": "^3.1.0",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^6.5.4"
  }
}
